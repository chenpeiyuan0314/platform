<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration 
      PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
      "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry
    location="C:\Users\Administrator\.m2\repository\mysql\mysql-connector-java\5.1.38\mysql-connector-java-5.1.38.jar" />

  <context id="MySql" targetRuntime="MyBatis3" defaultModelType="flat">
    <commentGenerator>
      <property name="" value=""/>
      <property name="suppressAllComments" value="true" />
    </commentGenerator>

    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
      connectionURL="jdbc:mysql://localhost:3306/mine?characterEncoding=utf8"
      userId="root" password="root">
    </jdbcConnection>

    <javaTypeResolver>
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <!-- entity -->
    <javaModelGenerator targetPackage="com.mybatis.model"
      targetProject="src/main/java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
    <!-- mapping -->
    <sqlMapGenerator targetPackage="sqlmap"
      targetProject="src/main/resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
    <!-- client -->
    <javaClientGenerator type="XMLMAPPER"
      targetPackage="com.mybatis.mapper" targetProject="src/main/java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>
    
    <!--  -->
    <table tableName="%" enableCountByExample="false"
      enableSelectByExample="false" enableUpdateByExample="false"
      enableDeleteByExample="false">
    </table>
  </context>

</generatorConfiguration>